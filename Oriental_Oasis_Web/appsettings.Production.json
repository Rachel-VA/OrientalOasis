{
  "ConnectionStrings": {
    "DefaultConnection": "Server=${{ secrets.AZURE_KEY }};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Stripe": {
    "SecretKey": "${{ secrets.STRIPE_SECRET_KEY }}",
    "PublisharableKey": "${{ secrets.STRIPE_PUBLISH_KEY }}",
    "FromEmailPassword": "your-email-password"
  }
}
